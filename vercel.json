{
  "version": 2,
  "buildCommand": "npm run vercel:build --prefix polymet-swiss-driving-school/frontend",
  "outputDirectory": "polymet-swiss-driving-school/frontend/dist",
  "installCommand": "npm install && cd polymet-swiss-driving-school && npm install && cd frontend && npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  }
}